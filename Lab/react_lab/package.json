{
  "name": "react_lab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev": "vite dev",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "cy:open": "cypress open",
    "test:e2e:dev": "start-server-and-test 'yarn dev' 'http-get://localhost:3000' 'yarn cy:open'",
    "cy:run": "cypress run",
    "ci": "cross-env PORT=1234 start-server-and-test 'yarn dev' 'http-get://localhost:1234' 'yarn cy:run'",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc -b",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\""
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.38.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/node": "^18.15.12",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@unocss/preset-wind": "^0.51.5",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "happy-dom": "^9.8.4",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4",
    "unocss": "^0.51.5",
    "vite": "^4.3.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
