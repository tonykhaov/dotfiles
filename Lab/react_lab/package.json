{
  "name": "react_lab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev": "vite dev",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "cy:open": "cypress open",
    "test:e2e:dev": "start-server-and-test 'yarn dev' 'http-get://localhost:3000' 'yarn cy:open'",
    "cy:run": "cypress run",
    "ci": "cross-env PORT=1234 start-server-and-test 'yarn dev' 'http-get://localhost:1234' 'yarn cy:run'",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc -b",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\"",
    "prepare": "panda codegen"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.38.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@pandacss/dev": "^0.34.3",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "happy-dom": "^13.8.2",
    "jsdom": "^24.0.0",
    "msw": "^2.2.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
